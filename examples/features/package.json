{
  "name": "contexify-features-examples",
  "description": "Standalone examples to illustrate various features provided by contexify",
  "version": "1.0.0",
  "type": "module",
  "keywords": ["contexify", "context", "example", "tutorial"],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "teomyth <teomyth@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/teomyth/contexify.git",
    "directory": "examples/features"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "fix": "biome check --write . && biome format --write .",
    "test": "vitest run",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "contexify": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
